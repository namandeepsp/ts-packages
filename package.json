{
  "name": "ts-packages",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish-cache-pkg": "pnpm --filter ./packages/cache build && pnpm --filter ./packages/cache publish",
    "publish-error-pkg": "pnpm --filter ./packages/errors-utils build && pnpm --filter ./packages/errors-utils publish",
    "publish-js-extensions-pkg": "pnpm --filter ./packages/js-extensions build && pnpm --filter ./packages/js-extensions publish",
    "publish-response-pkg": "pnpm --filter ./packages/response-utils build && pnpm --filter ./packages/response-utils publish",
    "publish-server-pkg": "pnpm --filter ./packages/server-utils build && pnpm --filter ./packages/server-utils publish",
    "publish-security-pkg": "pnpm --filter ./packages/security build && pnpm --filter ./packages/security publish",
    "build": "pnpm --filter ./packages/cache build && pnpm --filter ./packages/errors-utils build && pnpm --filter ./packages/js-extensions build && pnpm --filter ./packages/response-utils build && pnpm --filter ./packages/server-utils build && pnpm --filter ./packages/security build",
    "publish-all-pkg": "pnpm publish-server-pkg && pnpm publish-response-pkg && pnpm publish-error-pkg && pnpm publish-js-extensions-pkg && pnpm publish-security-pkg && pnpm publish-cache-pkg",
    "clear-cache": "pnpm store prune",
    "organize": "biome check packages/*/src --write",
    "organize-imports": "biome check packages/*/src --write",
    "format-all": "biome format packages/*/src --write",
    "lint-all": "biome check packages/*/src",
    "organize-all": "biome check packages/*/src --write",
    "organize-package": "biome check packages/*/src --write --files-filter $npm_config_package",
    "format-package": "biome format packages/*/src --write --files-filter $npm_config_package",
    "lint-package": "biome check packages/*/src --files-filter $npm_config_package",
    "clean": "biome check packages/*/src --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "typescript": "^5.9.3"
  }
}