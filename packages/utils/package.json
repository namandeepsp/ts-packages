{
  "name": "@naman_deep_singh/utils",
  "version": "2.6.2",
  "description": "Universal JavaScript prototype extensions for common development utilities",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./array": {
      "types": "./dist/types/array/index.d.ts",
      "import": "./dist/esm/array/index.js",
      "require": "./dist/cjs/array/index.js"
    },
    "./number": {
      "types": "./dist/types/number/index.d.ts",
      "import": "./dist/esm/number/index.js",
      "require": "./dist/cjs/number/index.js"
    },
    "./string": {
      "types": "./dist/types/string/index.d.ts",
      "import": "./dist/esm/string/index.js",
      "require": "./dist/cjs/string/index.js"
    },
    "./object": {
      "types": "./dist/types/object/index.d.ts",
      "import": "./dist/esm/object/index.js",
      "require": "./dist/cjs/object/index.js"
    },
    "./core": {
      "types": "./dist/types/core/index.d.ts",
      "import": "./dist/esm/core/index.js",
      "require": "./dist/cjs/core/index.js"
    },
    "./utils": {
      "types": "./dist/types/utils/index.d.ts",
      "import": "./dist/esm/utils/index.js",
      "require": "./dist/cjs/utils/index.js"
    },
    "./init": {
      "types": "./dist/types/init/index.d.ts",
      "import": "./dist/esm/init/index.js",
      "require": "./dist/cjs/init/index.js"
    },
    "./extensions": {
      "types": "./dist/types/extensions/index.d.ts",
      "import": "./dist/esm/extensions/index.js",
      "require": "./dist/cjs/extensions/index.js"
    },
    "./errors": {
      "types": "./dist/types/errors/index.d.ts",
      "import": "./dist/esm/errors/index.js",
      "require": "./dist/cjs/errors/index.js"
    },
    "./types": {
      "types": "./dist/types/types/index.d.ts",
      "import": "./dist/esm/types/index.js",
      "require": "./dist/cjs/types/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm run build:types && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "clean:js": "find src -type f -name \"*.js\" -delete"
  },
  "keywords": [
    "javascript",
    "utilities",
    "extensions",
    "prototype",
    "string",
    "array",
    "object",
    "universal"
  ],
  "author": "Naman Deep Singh",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "rimraf": "^5.0.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@naman_deep_singh/errors": "^2.3.1",
    "@types/node": "^25.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}