{
  "name": "ts-packages",
  "version": "5.2.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish-cache-pkg": "pnpm --filter ./packages/cache build && pnpm --filter ./packages/cache publish",
    "publish-communication-core-pkg": "pnpm --filter ./packages/communication/core build && pnpm --filter ./packages/communication/core publish",
    "publish-communication-protocols-pkg": "pnpm --filter ./packages/communication/protocols build && pnpm --filter ./packages/communication/protocols publish",
    "publish-error-pkg": "pnpm --filter ./packages/errors build && pnpm --filter ./packages/errors publish",
    "publish-utils-pkg": "pnpm --filter ./packages/utils build && pnpm --filter ./packages/utils publish",
    "publish-response-pkg": "pnpm --filter ./packages/http-response build && pnpm --filter ./packages/http-response publish",
    "publish-server-pkg": "pnpm --filter ./packages/server build && pnpm --filter ./packages/server publish",
    "publish-security-pkg": "pnpm --filter ./packages/security build && pnpm --filter ./packages/security publish",
    "build": "pnpm --filter ./packages/cache build && pnpm --filter ./packages/communication/core build && pnpm --filter ./packages/communication/protocols build && pnpm --filter ./packages/errors build && pnpm --filter ./packages/utils build && pnpm --filter ./packages/http-response build && pnpm --filter ./packages/server build && pnpm --filter ./packages/security build",
    "publish-all-pkg": "pnpm publish-server-pkg && pnpm publish-response-pkg && pnpm publish-error-pkg && pnpm publish-utils-pkg && pnpm publish-security-pkg && pnpm publish-cache-pkg && pnpm publish-communication-core-pkg && pnpm publish-communication-protocols-pkg",
    "clear-cache": "pnpm store prune",
    "organize": "biome check packages/*/src --write",
    "organize-imports": "biome check packages/*/src --write",
    "format-all": "biome format packages/*/src --write",
    "lint-all": "biome check packages/*/src",
    "organize-all": "biome check packages/*/src --write",
    "organize-package": "biome check packages/*/src --write --files-filter $npm_config_package",
    "format-package": "biome format packages/*/src --write --files-filter $npm_config_package",
    "lint-package": "biome check packages/*/src --files-filter $npm_config_package",
    "clean": "biome check packages/*/src --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "typescript": "^5.9.3"
  }
}